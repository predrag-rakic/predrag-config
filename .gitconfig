[user]
	name = Predrag Rakic
[alias]
	co  = checkout
	mas = checkout master
	d   = diff
	l   = log --decorate --graph --all -4
	st  = status
	fe  = fetch
	br  = branch
	cm  = commit -a -m
	wip = commit -a -m "WIP"
	po  = push origin
	ll  = "log --format=format:'%C(red)%h%C(reset) %C(green)%an%C(reset) %C(yellow)(%ar)%C(reset) %C(blue)%d%C(reset) %C(white)%s%C(reset)' --abbrev-commit --date=relative --graph --all -12"

[push]
	default = current
